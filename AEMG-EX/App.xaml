<Application x:Class="AEMG_EX.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AEMG_EX"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/Color.xaml"/>
                <ResourceDictionary Source="/Styles/Image.xaml"/>
                <ResourceDictionary Source="/Styles/Button.xaml"/>
                <ResourceDictionary Source="/Styles/ComboBox.xaml"/>
                <ResourceDictionary Source="/Styles/List.xaml"/>
                <ResourceDictionary Source="/Styles/ScrollViewer.xaml"/>
                <ResourceDictionary Source="/Styles/Tab.xaml"/>
                <ResourceDictionary Source="/Styles/ToolTip.xaml"/>
                <ResourceDictionary Source="/Styles/Animation.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
